package FileAction.GetData;

import org.json.JSONArray;
import org.json.JSONObject;

public class GetAllJSONData extends FileActionGetLines
{

	public GetAllJSONData(String i_Path)
	{
		super(i_Path);
	}

	@Override
	protected void doOnEveryLine(JSONObject i_CurrentJSONObject, JSONArray i_Array)
	{
		i_Array.put(i_CurrentJSONObject);
	}
	
}
